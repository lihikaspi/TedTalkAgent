{
  "version": 2,
  "functions": {
  "api/main.py": {
    "runtime": "@vercel/python",
    "excludeFiles": "{.venv/**,talk_indexer.py,summary_generator.py,app.py}"
  }
  },
  "rewrites": [
    {
    "source": "/api/(.*)",
    "destination": "api/main.py"
    },
    {
    "source": "/",
    "destination": "/index.html"
    }
  ]
}